<blankWindow:BlankWindow x:Class="MMC.MSpace.Views.UserMessagesView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MMC.MSpace.Views"
		xmlns:blankWindow="http://GFramework.org"
        mc:Ignorable="d" ShowInTaskbar="False"
        Title="UserMessageView" Height="95" Width="130" ResizeMode="NoResize" Background="#1E2026">
	<Grid>
		<StackPanel HorizontalAlignment="Center" Orientation="Vertical" Margin="0,5,0,0">
			<TextBlock x:Name="deadLine" Height="20" Width="120" FontSize="14" Foreground="#B3BBC5" HorizontalAlignment="Center" VerticalAlignment="Top" Background="#1E2026" Text="    有效期还剩0天" Margin="3" />
			<Button x:Name="myReport" Height="30" Width="125"   Background="#1E2026" Foreground="White" Command="{Binding ReportCenterCmd}"   Content="报告中心"/>
			<Button x:Name="setReport" Height="30"  Width="125"  Background="#1E2026" Foreground="White" Command="{Binding ReportSettingCommand}"  Content="报告设置"/>
		</StackPanel>
	</Grid>
</blankWindow:BlankWindow>
