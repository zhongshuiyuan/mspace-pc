                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="tb1" Property="Foreground" Value="White"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

