                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge" />
                            <ImageDim name="GardeniaSkin/TooltipRightEdge" dimension="Width" />
                        </OperatorDim>
                    </Dim>
