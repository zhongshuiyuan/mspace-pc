<Window x:Class="Mmc.Mspace.ToolModule.DynamicClip.ClipRename"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mmc.Mspace.ToolModule.DynamicClip"
        mc:Ignorable="d"
        Title="ClipRename" Height="450" Width="800">
	<Grid>
		<TextBox HorizontalAlignment="Center" Height="23"  TextWrapping="Wrap"  VerticalAlignment="Center" Width="120" Text="{Binding Clipname}"/>
		<Button HorizontalAlignment="Center" Margin="0,200,0,0" VerticalAlignment="Center" Width="75">
			<Button Content="Button" Width="75" Command="{Binding GetName}" />
		</Button>

	</Grid>
</Window>
